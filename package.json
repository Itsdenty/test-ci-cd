{
  "name": "test-ci-cd",
  "version": "1.0.0",
  "description": "for testing ci-cd",
  "main": "index.js",
  "scripts": {
    "start-dev": "nodemon --exec babel-node server/index.js",
    "build": "babel server --copy-files --out-dir dist",
    "start": "node dist/index.js",
    "heroku-postbuild": "npm run build",
    "serve": "node dist/index.js",
    "test": "nyc mocha --timeout 20000 -r @babel/register server/test.js --exit ",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Itsdenty/test-ci-cd.git"
  },
  "author": "Abd-afeez Abd-hamid",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Itsdenty/test-ci-cd/issues"
  },
  "homepage": "https://github.com/Itsdenty/test-ci-cd#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "mailgun-js": "^0.22.0",
    "morgan": "^1.9.1",
    "rimraf": "^2.6.3",
    "swagger-jsdoc": "^3.2.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "mocha": "^6.1.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.11",
    "nyc": "^13.3.0",
    "supertest": "^4.0.2"
  }
}
